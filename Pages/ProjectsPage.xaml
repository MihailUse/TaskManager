<Page x:Class="TaskManager.Pages.ProjectsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TaskManager.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" 
      d:DesignWidth="800"
      Title="ProjectsPage"
      Style="{StaticResource WinStyle}">

    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>

        <Frame x:Name="WorkSpaceFrame" NavigationUIVisibility="Hidden" Grid.Row="0"/>

        <TextBlock Grid.Row="1">
            <Hyperlink Click="CreateProjectButton_Click">Create Project</Hyperlink>
        </TextBlock>

    </Grid>
</Page>
